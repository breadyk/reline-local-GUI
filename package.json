{
  "private": true,
  "name": "relin-local-gui",
  "version": "0.0.9",
  "main": "index-electron.cjs",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "vite preview",
    "typecheck": "tsc",
    "lint": "biome lint --write --unsafe .",
    "format": "biome format --write --no-errors-on-unmatched .",
    "biome": "pnpm format && pnpm lint",
    "electron": "electron index-electron.cjs",
    "electron-dev": "wait-on http://localhost:5173 && electron index-electron.cjs",
    "go": "concurrently \"npm run dev\" \"npm run electron-dev\"",
    "dist": "vite build && electron-builder",
    "distlin": "vite build && electron-builder --linux",
    "distmac": "vite build && electron-builder --mac"
  },
  "dependencies": {
    "@fontsource/geist-mono": "^5.0.3",
    "@fontsource/geist-sans": "^5.0.3",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.417.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tree-kill": "^1.2.2",
    "ts-node": "^10.9.2",
    "unzipper": "^0.12.3",
    "wait-on": "^8.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@remix-run/dev": "^2.13.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.5",
    "electron": "23.1.3",
    "electron-builder": "^26.0.12",
    "postcss": "^8.4.41",
    "react-router-dom": "^6.25.1",
    "tailwindcss": "^3.4.10",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.2",
    "unplugin-fonts": "^1.1.1",
    "vite": "^5.3.4",
    "vite-tsconfig-paths": "^4.3.2",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.brdk.reline-local-gui",
    "productName": "reline-local-gui",
    "files": [
      "dist/**",
      "index-electron.cjs",
      "preload.js",
      "package.json",
      {
        "from": "reline",
        "to": "reline",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [],
    "asar": false,
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
